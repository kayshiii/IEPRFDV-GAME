    content: Whatever happens today, it wasn't personal. I tried to warn you.
    emailType: 1
    basicResponse:
      choiceText: Mark as read.
      sentienceModifier: 0
      dependencyModifier: 0
      resultDescription: basic
    enhancedResponse:
